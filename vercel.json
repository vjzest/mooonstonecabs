{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/public" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "status": 404, "headers": { "x-vercel-proxy": "disabled" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
