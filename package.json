{
  "name": "moonstone-cabs",
  "version": "1.0.0",
  "private": true,
  "description": "Moonstone Cabs - Premium Taxi & Transportation Service",
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=client\" \"npm run dev --workspace=server\"",
    "build": "npm run build --workspace=client && npm run build --workspace=server",
    "start": "npm start --workspace=server",
    "server:dev": "npm run dev --workspace=server",
    "client:dev": "npm run dev --workspace=client",
    "server:build": "npm run build --workspace=server",
    "client:build": "npm run build --workspace=client"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "devDependencies": {
    "concurrently": "^9.0.0"
  },
  "keywords": [
    "taxi",
    "transportation",
    "ride-sharing",
    "booking"
  ],
  "author": "Moonstone Cabs",
  "license": "ISC"
}
